{
  "compilerOptions": {
    "module": "amd",
    "noImplicitAny": true,
    "sourceMap": true,
    "jsx": "react",
    "reactNamespace": "jsxFactory",
    "target": "es5",
    "experimentalDecorators": true,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "dist/js",
    "types": ["arcgis-js-api", "dojo", "es6-shim"]
  },
  "include": [
    "src/js/**/*",
    "node_modules/rxjs/**/*.d.ts"
  ]
}
