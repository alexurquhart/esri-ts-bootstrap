{
  "compilerOptions": {
    "module": "amd",
    "noImplicitAny": true,
    "sourceMap": true,
    "jsx": "react",
    "reactNamespace": "jsxFactory",
    "target": "es5",
    "experimentalDecorators": true,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "dist/js",
    "types": [ "arcgis-js-api", "dojo" ]
  },
  "include": [
    "src/js/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
